<Window x:Uid="Window_1" x:Class="UVOutliner.UnhandledException.wnd_ExceptionDetails"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="Exception Details" 
        ResizeMode="CanResizeWithGrip"
        Height="268" Width="601"
        WindowStartupLocation="CenterOwner">
    <Grid x:Uid="Grid_1" Margin="20">
        <Grid.RowDefinitions>
            <RowDefinition x:Uid="RowDefinition_1" Height="*"/>
            <RowDefinition x:Uid="RowDefinition_2" Height="Auto"/>
        </Grid.RowDefinitions>
        <TextBox x:Uid="tbException" BorderThickness="0" VerticalScrollBarVisibility="Auto" AcceptsReturn="True" Name="tbException"></TextBox>
        <Button x:Uid="Button_1" Height="23" Grid.Row="1" Width="70" Margin="0,8,0,0" HorizontalAlignment="Right" IsCancel="True" IsDefault="True">Close</Button>
    </Grid>
</Window>
